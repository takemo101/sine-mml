{
	"mcpServers": {
		"sequential-thinking": {
			"command": "npx",
			"args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
		},
		"serena": {
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"--context",
				"ide-assistant"
			]
		},
		"container-use": {
			"command": "container-use",
			"args": ["stdio"]
		},
		"playwright": {
			"command": "npx",
			"args": ["-y", "@playwright/mcp@latest"],
			"disabled": true
		}
	}
}
